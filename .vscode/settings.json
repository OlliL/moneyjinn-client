{
  "eslint.validate": ["vue", "javascript", "javascriptreact"],
  "editor.codeActionsOnSave": {
    "source.fixAll.eslint": true
  },
  "editor.formatOnPaste": true,
  "editor.formatOnType": true,
  "editor.formatOnSave": false,
  "files.eol": "\n",
  "search.exclude": {
    "**/node_modules": false
  }
}
